<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.alexdroid.contractiontimer"
	android:versionCode="1"
	android:versionName="1.0">
	<uses-sdk android:minSdkVersion="3"
		android:targetSdkVersion="8" />
	<application android:label="@string/contraction_timer_name"
		android:icon="@drawable/icon"
		android:debuggable="true">
		<activity android:name="ContractionTimer"
			android:label="@string/contraction_timer_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name="ContractionList"
			android:label="@string/contraction_list_name">
			<intent-filter>
				<action android:name="android.intent.action.VIEW" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
		<activity android:name="ContractionGraph"
			android:label="@string/contraction_graph_name">
			<intent-filter>
				<action android:name="android.intent.action.VIEW" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
	</application>
</manifest> 
